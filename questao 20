#include <stdio.h>

void multiplo() {
    int a, b;

    // Lê os dois inteiros
    printf("Digite o primeiro número: ");
    scanf("%d", &a);
    printf("Digite o segundo número: ");
    scanf("%d", &b);

    // Calcula o resto da divisão e imprime o resultado
    printf("O primeiro número é múltiplo do segundo: %s\n", (a % b == 0) ? "true" : "false");
}

int main() {
    multiplo();
    return 0;
}
