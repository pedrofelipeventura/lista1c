#include <stdio.h>

void caracteres() {

    // Conjunto de caracteres a serem impressos
        
    char caracteres[] = {'A', 'B', 'C', 'a', 'b', 'c', '0', '1', '2', '$', '*', '+', '/', ' '};
        int tamanho = sizeof(caracteres) / sizeof(caracteres[0]);

        // Imprime os caracteres e seus valores inteiros equivalentes
        for (int i = 0; i < tamanho; i++) {
            printf("Caractere: '%c' - CÃ³digo inteiro: %d\n", caracteres[i], caracteres[i]);
        }
}
int main() {
    caracteres();
    return 0;
}
