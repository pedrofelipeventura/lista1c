#include <stdio.h>
//troca de valores 
 
void trocadevalores(){
  int A, B, C;
  printf("Digite um valor para A:\n");
  scanf("%d", &A);
  printf("Digite um valor para B:\n");
  scanf("%d", &B);
  C = A;
  A = B;
  B = C;
  printf("O valor de A é: %d\n", A);
  printf("O valor de B é: %d\n", B);
  
}


int main(void) {
  trocadevalores();
  return 0;
}

