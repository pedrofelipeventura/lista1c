#include <stdio.h>

 
void numeros(){
  int num1,num2;
  printf("Digite o primeiro número:\n");
  scanf("%d",&num1);
  printf("DIgite o segundo número:\n");
  scanf("%d",&num2);
  printf("A soma do primeiro com o segundo é: %d\n", num1+num2);
  printf("O produto do primeiro com segundo é igual a: %d\n", num1*num2);
  printf("A diferença do primeiro pelo segundo é: %d\n", num1-num2);
  printf("O quociente encontrado pela divisão do primeiro pelo segundo é: %d\n", num1/num2);
  printf("O resto da divisão do número 1 pelo número 2 é igual a: %d\n", num1 % num2);
}


int main(void) {
  numeros();
  return 0;
}
